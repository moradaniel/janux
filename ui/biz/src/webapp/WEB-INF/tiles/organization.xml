<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 1.1//EN"
	"http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd"
>
<tiles-definitions>

	<!-- Default main L-nav template -->
	<definition name="MainLayout" page="LayoutMain.vm">
		<put name="title" value="Janux Biz" type="string"/>
	</definition>

	<!-- extend other views from the main layout_main -->
	<definition name="Description" extends="MainLayout">
		<put name="content" value="organization/Description.vm" type="page"/>
	</definition>

	<definition name="" extends="Description"/>

	<definition name="ContactInfo" extends="MainLayout">
		<put name="content" value="organization/ContactInfo.vm" type="page"/>
	</definition>

</tiles-definitions>
